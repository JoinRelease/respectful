<table class="table">
  <thead>
  <tr>
    <th>Created At</th>
    <th>Name</th>
    <th>Email</th>
    <th>From Lead?</th>
    <th colspan="3"></th>
  </tr>
  </thead>

  <tbody>
  <% @users.each do |user| %>
    <tr>
      <td><%= user.created_at.strftime("%Y/%m/%d") %></td>
      <td><%= user.full_name %></td>
      <td><%= user.email %></td>
      <td><% if user.lead.present? %>
            <i class="fa fa-check"></i>
          <% end %>
      </td>
      <td><%= link_to '', admin_user_path(user), class: 'fa fa-binoculars', title: 'View' %> </td>
      <td><%= link_to '', edit_admin_user_path(user), class: 'fa fa-pencil-square-o', title: 'Edit' %> </td>
    </tr>
  <% end %>
  </tbody>
</table>

<%= paginate @users %>
